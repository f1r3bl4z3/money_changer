<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.moneychanger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyChanger">
        <activity
            android:name=".ChooseDealerActivity"
            android:label="@string/title_activity_choose_dealer"
            android:theme="@style/Theme.MoneyChanger.NoActionBar"></activity>
        <activity android:name=".OrdersActivity" />
        <activity android:name=".DealerActivity" />
        <activity android:name=".RequestorMainActivity" />
        <activity android:name=".Login.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.AuthenticationActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>